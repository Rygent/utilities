    "prepack": "pnpm run build",
    "bump": "cliff-jumper"
  },
  "devDependencies": {
    "@favware/cliff-jumper": "^2.2.3",
    "prettier": "^3.3.3",
    "tsup": "^7.2.0",
    "typescript": "^5.6.3"
  },
  "dependencies": {
    "colorette": "^2.0.20",
    "moment": "^2.30.1",
    "undici": "^5.28.4",
    "winston": "^3.17.0"
  },
  "homepage": "https://github.com/Rygent/Utilities/tree/main/packages/logger",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Rygent/Utilities.git",
    "directory": "packages/logger"
  },
  "bugs": {
    "url": "https://github.com/Rygent/Utilities/issues"
  },
  "files": [
    "./dist/**/*.js*",
    "./dist/**/*.mjs*",
    "./dist/**/*.d*"
