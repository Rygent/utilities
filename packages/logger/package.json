{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "@rygent/logger",
  "version": "1.0.0",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    "require": "./dist/index.js",
    "import": "./dist/index.mjs",
    "types": "./dist/index.d.ts"
  },
  "scripts": {
    "build": "tsup",
    "lint": "eslint \"src/**/*.ts\" --format=pretty",
    "lint:fix": "eslint \"src/**/*.ts\" --fix --format=pretty",
    "format": "prettier . --write",
    "format:check": "prettier . --check",
    "prepack": "pnpm run build",
    "bump": "cliff-jumper"
  },
  "license": "MIT",
  "keywords": [],
  "repository": {
    "type": "git",
    "url": "https://github.com/rygent/utilities.git",
    "directory": "packages/logger"
  },
  "bugs": {
    "url": "https://github.com/rygent/utilities/issues"
  },
  "funding": "https://github.com/rygent/utilities?sponsor",
  "dependencies": {
    "colorette": "^2.0.20",
    "moment": "^2.30.1",
    "undici": "^5.28.4",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@favware/cliff-jumper": "^2.2.3",
    "eslint": "^9.14.0",
    "eslint-formatter-pretty": "^6.0.1",
    "prettier": "^3.3.3",
    "tsup": "^7.2.0",
    "typescript": "^5.6.3"
  },
  "files": [
    "./dist/**/*.js*",
    "./dist/**/*.mjs*",
    "./dist/**/*.d*"
  ],
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "engines": {
    "node": ">=18.12.0"
  },
  "packageManager": "pnpm@9.13.0"
}
