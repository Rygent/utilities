{
  "name": "@rygent/anilist",
  "version": "1.4.0",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    "require": "./dist/index.js",
    "import": "./dist/index.mjs",
    "types": "./dist/index.d.ts"
  },
  "scripts": {
    "test": "vitest run",
    "lint": "eslint src --ext ts --fix -c ../../.eslintrc",
    "format": "prettier . --write",
    "format:check": "prettier . --check",
    "build": "tsup",
    "prepack": "pnpm run build",
    "bump": "cliff-jumper"
  },
  "devDependencies": {
    "@favware/cliff-jumper": "^2.2.3",
    "@types/he": "^1.2.3",
    "@vitest/coverage-v8": "^0.34.6",
    "prettier": "^3.3.3",
    "tsup": "^7.2.0",
    "typescript": "^5.6.3",
    "vitest": "^0.34.6"
  },
  "dependencies": {
    "axios": "^1.7.7",
    "graphql": "^16.9.0",
    "he": "^1.2.0"
  },
  "files": [
    "./dist/**/*.js*",
    "./dist/**/*.mjs*",
    "./dist/**/*.d*"
  ],
  "engines": {
    "node": ">=18.12.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "packageManager": "pnpm@9.13.0"
}
