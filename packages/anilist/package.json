{
	"name": "@rygent/anilist",
	"version": "1.4.0",
	"description": "An UNOFFICIAL wrapper for Anilist GraphQL API written in typescript.",
	"main": "./dist/index.js",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"exports": {
		"import": {
			"types": "./dist/index.d.mts",
			"default": "./dist/index.mjs"
		},
		"require": {
			"types": "./dist/index.d.ts",
			"default": "./dist/index.js"
		}
	},
	"scripts": {
		"test": "vitest run",
		"lint": "eslint src --ext ts --fix -c ../../.eslintrc",
		"codegen": "graphql-codegen --config codegen.ts",
		"build": "tsup",
		"prepack": "yarn build",
		"bump": "cliff-jumper"
	},
	"devDependencies": {
		"@favware/cliff-jumper": "^2.2.1",
		"@graphql-codegen/cli": "^5.0.0",
		"@graphql-codegen/typescript": "^4.0.1",
		"@vitest/coverage-v8": "^0.34.6",
		"graphql": "^16.8.1",
		"tsup": "^7.2.0",
		"typescript": "^5.2.2",
		"vitest": "^0.34.6"
	},
	"dependencies": {
		"html-entities": "^2.4.0"
	},
	"files": [
		"./dist/**/*.js*",
		"./dist/**/*.mjs*",
		"./dist/**/*.d*"
	],
	"engines": {
		"node": ">=18.12.0"
	},
	"keywords": [
		"anilist",
		"wrapper",
		"api",
		"anilist-api"
	],
	"publishConfig": {
		"access": "public"
	},
	"packageManager": "yarn@4.0.1"
}
