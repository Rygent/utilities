{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "@rygent/spotify",
  "version": "1.1.1",
  "description": "A simple to use API library for the Spotify REST API.",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    "require": "./dist/index.js",
    "import": "./dist/index.mjs",
    "types": "./dist/index.d.ts"
  },
  "scripts": {
    "test": "vitest run",
    "build": "tsup",
    "lint": "eslint src --ext ts --fix -c ../../.eslintrc",
    "format": "prettier . --write",
    "format:check": "prettier . --check",
    "prepack": "pnpm run build",
    "bump": "cliff-jumper"
  },
  "license": "MIT",
  "keywords": [],
  "repository": {
    "type": "git",
    "url": "https://github.com/rygent/utilities.git",
    "directory": "packages/spotify"
  },
  "bugs": {
    "url": "https://github.com/rygent/utilities/issues"
  },
  "funding": "https://github.com/rygent/utilities?sponsor",
  "dependencies": {
    "undici": "^5.28.4"
  },
  "devDependencies": {
    "@favware/cliff-jumper": "^2.2.3",
    "@vitest/coverage-v8": "^0.34.6",
    "prettier": "^3.3.3",
    "tsup": "^7.2.0",
    "typescript": "^5.6.3",
    "vitest": "^0.34.6"
  },
  "files": [
    "./dist/**/*.js*",
    "./dist/**/*.mjs*",
    "./dist/**/*.d*"
  ],
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "engines": {
    "node": ">=18.12.0"
  },
  "packageManager": "pnpm@9.13.0"
}
