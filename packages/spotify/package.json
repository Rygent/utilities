{
  "name": "@rygent/spotify",
  "version": "1.1.1",
  "description": "A simple to use API library for the Spotify REST API.",
  "license": "MIT",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    "require": "./dist/index.js",
    "import": "./dist/index.mjs",
    "types": "./dist/index.d.ts"
  },
  "scripts": {
    "test": "vitest run",
    "lint": "eslint src --ext ts --fix -c ../../.eslintrc",
    "build": "tsup",
    "prepack": "yarn build",
    "bump": "cliff-jumper"
  },
  "devDependencies": {
    "@favware/cliff-jumper": "^2.1.2",
    "@vitest/coverage-c8": "^0.31.4",
    "tsup": "^6.7.0",
    "typescript": "^4.9.5",
    "vitest": "^0.31.4"
  },
  "dependencies": {
    "undici": "^5.23.0"
  },
  "homepage": "https://github.com/Rygent/Utilities/tree/main/packages/spotify",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Rygent/Utilities.git",
    "directory": "packages/spotify"
  },
  "bugs": {
    "url": "https://github.com/Rygent/Utilities/issues"
  },
  "files": [
    "./dist/**/*.js*",
    "./dist/**/*.mjs*",
    "./dist/**/*.d*"
  ],
  "engines": {
    "node": ">=16.8.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "packageManager": "yarn@3.6.3"
}
