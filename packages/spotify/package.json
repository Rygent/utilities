{
	"name": "@rygent/spotify",
	"version": "1.1.1",
	"description": "An UNOFFICIAL wrapper for Spotify REST API written in typescript.",
	"license": "MIT",
	"main": "./dist/index.js",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"exports": {
        "import": {
            "types": "./dist/index.d.mts",
            "default": "./dist/index.mjs"
        },
		"require": {
            "types": "./dist/index.d.ts",
            "default": "./dist/index.js"
        }
	},
	"scripts": {
		"test": "vitest run",
		"lint": "eslint src --ext ts --fix -c ../../.eslintrc",
		"build": "tsup",
		"prepack": "yarn build",
		"bump": "cliff-jumper"
	},
	"devDependencies": {
		"@favware/cliff-jumper": "^2.2.1",
		"@vitest/coverage-v8": "^0.34.6",
		"tsup": "^7.2.0",
		"typescript": "^5.2.2",
		"vitest": "^0.34.6"
	},
	"homepage": "https://github.com/Rygent/Utilities/tree/main/packages/spotify",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Rygent/Utilities.git",
		"directory": "packages/spotify"
	},
	"bugs": {
		"url": "https://github.com/Rygent/Utilities/issues"
	},
	"files": [
		"./dist/**/*.js*",
		"./dist/**/*.mjs*",
		"./dist/**/*.d*"
	],
	"engines": {
		"node": ">=18.12.0"
	},
    "keywords": [
        "spotify",
        "wrapper",
        "api",
        "spotify-api"
    ],
	"publishConfig": {
		"access": "public"
	},
	"packageManager": "yarn@4.0.1"
}
